{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "branchPrefix": "konflux/osp-mintmaker/",
  "enabledManagers": [
    "tekton",
    "dockerfile",
    "rpm-lockfile"
  ],
  "addLabels": [
    "/approve",
    "/lgtm",
    "konflux",
    "mintmaker"
  ],
  "packageRules": [
    {
      "description": " {{.Name}} - Automerge non-major updates",
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    }
  ]
}
